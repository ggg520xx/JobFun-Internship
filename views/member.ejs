                <% }else{ %>
                <img src="img/memberavatar/defaultphoto.png" id="photo" style="height: 100px;width: 100px;">
                <% } %>
                <input type="text" id="memberInfoId" class="d-none" value="<%= memberInfo[0].memberId %>">
                <input type="file" id="file" accept="image/gif, image/jpeg, image/png">
                <label for="file" id="uploadBtn">Choose Photo</label>
              </div>
            </li>
            <li class="userName" style="border: none;margin-top: 10px;">
              <h5 id="ss"> <%= memberInfo[0].memberName %> </h5>
