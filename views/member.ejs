            <h3 class="section-title-member">會員基本資料</h3>
            <div class="services-item  fadeInDown" style="color: black;">
              <div class="row">
                <div class="col-1">

                </div>
                <div class="col-5" style="text-align: left;margin:auto">
                  <div class="text-center">
                    <!-- <h3>個人資料</h3> -->
                  </div>
                  <br>

                  <h5 class="resumeDetail ml-5">姓&emsp;&emsp;名：</h5>
                  <h6 class="resumeDetail"><%= memberInfo[0].memberName %></h6>
                  <hr>
                  <h5 class="resumeDetail ml-5">電&emsp;&emsp;話：</h5>&emsp13;
                  <h6 class="resumeDetail"><%= memberInfo[0].memberPhone %></h6>
                  <hr>
                  <h5 class="resumeDetail ml-5">信&emsp;&emsp;箱：</h5>&emsp13;
                  <h6 class="resumeDetail"><%= memberInfo[0].memberEmail %></h6>
                  <hr>
                  <h5 class="resumeDetail ml-5">性&emsp;&emsp;別：</h5>&emsp13;
                  <h6 class="resumeDetail"><%= memberInfo[0].memberGender %></h6>


                </div>
                <div class="col-5" style="text-align: left;margin:auto">

                  <br>
                  <h5 class="resumeDetail ml-5">生&emsp;&emsp;日：</h5>&emsp13;
                  <h6 class="resumeDetail"><%= memberInfo[0].memberBirthday %></h6>
                  <hr>
                  <h5 class="resumeDetail ml-5">就讀大學：</h5>&emsp13;
                  <h6 class="resumeDetail"><%= memberInfo[0].schoolName %></h6>
                  <hr>
                  <h5 class="resumeDetail ml-5">科系類別：</h5>&emsp13;
                  <h6 class="resumeDetail"><%= memberInfo[0].majorClass %></h6>
                  <hr>
                  <h5 class="resumeDetail ml-5">就讀科系：</h5>&emsp13;
                  <h6 class="resumeDetail"><%= memberInfo[0].majorName %></h6>
                </div>


                <div class="col-1">

                </div>
              </div>
            </div>
            <br>

            <h3 class="section-title-member">為您推薦這些優質的實習職缺:</h3>
            <% randomJob.forEach(function(job){ %>
            <div class="row searchResultCard">
              <div class="col-lg-5">
                <div class="jobClessShow"><%= job.jobClassName %></div>
                <div><%= job.jobName %></div>
              </div>
              <div class="col-lg-3"><%= job.companyName%></div>
              <div class="col-lg-2"><%= job.cityName %><%= job.townshipName %></div>
              <div class="col-lg-2">
                <div class="applyBtnM mt-1">
                  <a href="/job?id=<%= job.jobId %>">
                      <div class="applyBtnTopM">
                          <div class="applyBtnPaperM"></div>
                          <a href="/job?id=<%= job.jobId %>">
                              <div class="applyBtnTextM">應徵</div>
                              
                          </a>

                      </div>
                      <div class="applyBtnSecM"></div>
                  </a>
              </div>
              </div>
            </div>
            <% }) %>
          </div>

          <div id="tab-2" class="clear" style="text-align: center;">
            <div class="tab-content" id="myTabContent">
              <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">

                <h3 class="section-title-member">新增與管理履歷</h3>
                <!-- 新增彈跳按鈕-->



















                <div class="col-12 text-right">
                  <button class="btn btn-common mb-4" style="display: inline-block;" id="check_click">新增履歷</button>
                </div>

                <!-- <div class="mapbtn">
                  <a href=""><img  src="../public/img/logo/logo.png" alt=""></a>
                </div> -->


                <div class="row">


                  <div class="col-12" id="hide_page">

                    <!-- <div class="member_tips">
                      <img src="img/member/tips_re2.png" alt="">
                    </div> -->


                    



                    <div id="resumeCard">


                      <% if(resume.length == "0"){  %>

                        <div class="text_member_tips">
                          <img src="img/member/tip_resume.png" alt="">
                        </div>
                      <% }else{  %>


                      <% resume.forEach(function(resume){ %>
                      <!-- 重要備註:id="resumeCard"這裡動態與editresume,addresume繫節 若要更動CSS或其他屬性 -->
                      <!-- 必須到public/js/ajaxrequest裡面 editresume.js,addresume.js一併修改 -->
                      <div class=" services-item-box fadeInDown">
                        <div class="card-body row ">
                          <div class="col-6 outercard">
                            <%= resume.resumeName %>
                          </div>

                          <div class="col-6">
                            <div style="float: right;">
                              <button class="btn btn-common" onclick="editResume('<%= resume.resumeId %>')"
                                id="edit_click">編輯</button>
                              <button class="btn btn-common" onclick="delResume('<%= resume.resumeId %>')"
                                id="delResumebtn">刪除</button>
                            </div>
                          </div>

                        </div>
                      </div>
                      <br>
                      <% }) %>


                      <% } %>
                    </div>
                  </div>


                  <div class="col-12" style="text-align: left;margin:auto" id="edit_resume" hidden="true">
                    <div class="services-item">
                      <h3 style="text-align: center;" class="section-title h3control">Step.1 基本資料修改</h3>

                      <div>
                        <input type="text" id="editResumeId" class="d-none">
                        <label for="" class="labelLay">履歷名稱：</label><input type="text" class="form-control editInputLay"
                          name="account" placeholder="" id="editResumeName">
                      </div>

                      <div>
                        <label for="" class="labelLay">聯絡電話：</label><input type="text" class="form-control editInputLay"
                          name="number" placeholder="" id="editResumePhone">
                      </div>

                      <div>
                        <label for="" class="labelLay">聯絡信箱：</label><input type="text" class="form-control editInputLay"
                          name="eMali" placeholder="" id="editResumeEmail">
                      </div>


                      <hr>
                      <h3 style="text-align: center;" class="section-title h3control">Step.2 技能填寫修改</h3>

                      <textarea name="" id="editResumeSkill" cols="30" rows="10" class="form-control"
                        style="width:50%; margin-left: auto;margin-right: auto;"></textarea>

                      <hr>
                      <h3 style="text-align: center;" class="section-title h3control">Step.3 重新上傳自傳</h3>

                      <input type="file" id="editResumePdf" name="filename" style="margin: auto;" accept=".pdf">
                      <br>
                      <div class="text-center">
                        <button id="previewEditresumeBtn" class="btn btn-common" data-toggle="modal"
                          data-target="#previewEditPdfModal">預覽自傳</button>
                      </div>
                      <br>
                      <div class=" mb-3 group-end " style="text-align:center">
                        <button class="btn btn-common mr-2" id="edit_back">&emsp;返回&emsp;</button>
                        <button class="btn btn-common mr-2" id="comfirmEdit">確認修改</button>
                      </div>
                    </div>
                  </div>






                  <div class="col-12" style="text-align: left;margin:auto" id="show_here" hidden="true">
                    <div class="services-item">

                      <h3 style="text-align: center;" class="section-title h3control">Step.1 基本資料填寫</h3>




                      <div>
                        <label for="" class="labelLay">履歷名稱：</label><input type="text" class="form-control editInputLay"
                          name="" placeholder="" id="addResumeName">
                      </div>

                      <div>
                        <label for="" class="labelLay">聯絡電話：</label><input type="text" class="form-control editInputLay"
                          name="" placeholder="" value="<%= memberInfo[0].memberPhone %>" id="addResumePhone">
                      </div>

                      <div>
                        <label for="" class="labelLay">聯絡信箱：</label><input type="text" class="form-control editInputLay"
                          name="" placeholder="" value="<%= memberInfo[0].memberEmail %>" id="addResumeEmail">
                      </div>


                      <hr>
                      <h3 style="text-align: center;" class="section-title h3control">Step.2 技能填寫</h3>

                      <textarea name="" id="addResumeSkill" cols="30" rows="10" class="form-control"
                        style="width:50%; margin-left: auto;margin-right: auto;"></textarea>
                      <hr>
                      <h3 style="text-align: center;" class="section-title h3control">Step.3 自傳上傳</h3>

                      <input type="file" id="addResumePdf" name="filename" style="margin: auto;" accept=".pdf">
                      <br>
                      <div class="text-center">
                        <button id="previewAddresumeBtn" class="btn btn-common d-none" data-toggle="modal"
                          data-target="#previewAddPdfModal">預覽自傳</button>
                      </div>
                      <br>





                      <div class=" mb-3 group-end " style="text-align:center">

                        <button class="btn btn-common mr-2" id="add_back">&emsp;返回&emsp;</button>
                        <button class="btn btn-common ml-2" id="addResumeComfirm">確認新增</button>
                      </div>



                      <div class="input-group mb-3 group-end">
                        <a class="filter btn btn-common btnModify" style="margin: auto;display: none;">確認修改</a>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <div class="tab-pane fade" id="profile" role="tabpanel" aria-labelledby="profile-tab">
                <br>
                <div class="row">
                  <div class="col-1"></div>
                  <div class="services-item fadeInDown col-3">
                    <h5 for="">First Resume</h5>
                    <br>
                    <div class="input-group mb-3 group-end">
                      <button type="button" class="filter btn btn-outline-common" data-toggle="modal"
                        data-target=".bd-example-modal-lg" style="margin: auto;color: black;">編輯
                      </button>
                    </div>
                    <div class="input-group mb-3 group-end">
                      <a class="filter btn btn-common" style="margin: auto;">刪除</a>
                    </div>
                    <br>





                  </div>

                </div>
              </div>
            </div>
          </div>





          <div id="tab-3">

            <h3 class="section-title-member">收藏職缺</h3>
            <div class="row" id="collectList">
              <% if(collect.length == "0"){  %>
                <div class="col-1"></div>
                <div class="text_member_tips col-10 text-center">
                  <img src="img/member/nocollect.png" alt="">
                </div>
                <div class="col-1"></div>
              <% }else{  %>
              <% collect.forEach(function(collect){ %>
              <!-- a set -->
              <div class="col-1"></div>
              <div class="services-item fadeInDown col-3 mb-4" style="padding: 20px;">
                <a href="/job?id=<%= collect.jobId %>">
                  <h5 class="collectlink"><%= collect.jobName %></h5>
                </a>
                <br>
                <h6><%= collect.companyName %></h6>
                <br>
                <h6><%= collect.cityName %><%= collect.townshipName %></h6>
                <br>
                <div class="input-group mb-3 group-end">
                  <a class="filter btn btn-common" style="margin: auto;" onclick="delCollect('<%= collect.collcetId %>')">取消收藏</a>
                </div>
              </div>
              <!-- a set end -->
              <% }) %>
              <% } %>


            </div>
          </div>









          <div id="tab-4">

            <h3 class="section-title-member">應徵紀錄</h3>
            <% if(jobApplyInfo.length == "0"){  %>
              <div class="text_member_tips text-center">
                <img src="img/member/noapply.png" alt="">
              </div>

            <% }else{ %>
            <table class="table table-hover" style="border: none; border-radius: 15px;">
              <thead>
                <tr>
                  <th scope="col" class="th_style">應徵日期</th>
                  <th scope="col" class="th_style">應徵實習</th>
                  <th scope="col" class="th_style">應徵公司</th>
                  <th scope="col" class="th_style">公司回覆</th>
                  <th scope="col" class="th_style">查看詳情</th>

                </tr>
              </thead>
              <tbody>


                <% jobApplyInfo.forEach(function(jobApply){ %>

                <tr>
                  <td><%= jobApply.applyDateF %></td>
                  <td><%= jobApply.jobName %></td>
                  <td><%= jobApply.companyName %></td>

                  <% if(jobApply.companyReply == "notviewed"){ %>
                  <td>尚未回覆</td>
                  <% }else if(jobApply.companyReply == "invite"){ %>
                  <td>已邀請面試</td>
                  <% }else{ %>
                  <td>已婉拒</td>
                  <% } %>
                  <td class="text-center"><button class="btn btn-common" data-toggle="modal" data-target="#replyMsgBox"
                      onclick="showMsg('<%= jobApply.jobAppyId%>')">查看詳細</button></td>

                </tr>

                <%  }) %>






              </tbody>
            </table>
            <% } %>
          </div>

          <div id="tab-5">
            <div class="row">
              <div class="col-3"></div>
              <div class="services-item fadeInDown fontSizeSet signLay" id="signFirstPart">

                <div class="services-content ">
                  <div class="text-center mb-4">
                    <h3>帳號修改</h3>
                  </div>
                  <div>
                    <input type="text" name="" id="editMemberId" value="<%= memberInfo[0].memberId %>" class="d-none">
                    <label for="" class="labelLay">帳&emsp;&emsp;號：</label><input type="text"
                      class="form-control signInputLay" name="account" placeholder=""
                      value="<%= memberInfo[0].memberAccount %>" disabled>
                  </div>

                  <div>
                    <label for="" class="labelLay">密&emsp;&emsp;碼：</label><input type="password"
                      class="form-control signInputLay" name="password" placeholder=""
                      value="<%= memberInfo[0].memberPassword %>" id="editPassword">
                  </div>

                  <div>
                    <label for="" class="labelLay">確認密碼：</label><input type="password" class="form-control signInputLay"
                      name="confirmPassword" placeholder="" value="<%= memberInfo[0].memberPassword %>"
                      id="editPasswordCom">
                  </div>



                  <div>
                    <label for="" class="labelLay">信&emsp;&emsp;箱：</label><input type="text"
                      class="form-control signInputLay" name="eMali" placeholder=""
                      value="<%= memberInfo[0].memberEmail %>" id="editEmail">
                  </div>

                  <div>
                    <label for="" class="labelLay">電&emsp;&emsp;話：</label><input type="text"
                      class="form-control signInputLay" name="phone" placeholder=""
                      value="<%= memberInfo[0].memberPhone %>" id="editPhone">
                  </div>

                  <div class="text-center">
                    <input type="button" class="btn btn-common" value="確認修改" id="editCom">
                  </div>
                </div>
              </div>
              <div class="col-3">

              </div>
            </div>

          </div>
        </div>


      </div>
  </div>
  </div>
  </div>
  </div>
  </main>
  </div>



