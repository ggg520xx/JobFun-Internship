                            <a class="nav-link" href="">
                                關於實習趣
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="">
                                聯絡我們
                            </a>
                        </li>
                        <li class="nav-item">
                            <% if( sessionUserCharacter == "company"){  %>
                            <a class="nav-link" href="/company">
                                <%= sessionUserName %> 會員中心
                            </a>
                        <li>
                            <a class="nav-link" href="/signout">
                                登出
                            </a>
                        </li>
                        <% }else if (sessionUserCharacter == "student"){ %>
                        <a class="nav-link" href="/member">
                            <%= sessionUserName %> 會員中心
                        </a>
                        <li>
                            <a class="nav-link" href="/signout">
                                登出
                            </a>
                        </li>
                        <% }else{ %>
                        <a class="nav-link" href="/signinstudent">
