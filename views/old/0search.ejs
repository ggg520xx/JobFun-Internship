        <div class="container">
            <br>
            <br>
            <br><br>


        </div>



        <!-- 搜尋列 -->
        <div class="searchBar" id="searchBar">
            <div class="container">
                <div class="searchBarLay">
                    <div class="searchCondition">
                        <form action="/searchresult" method="get">
                            <div class="keyWordInputLay">
                                <input class="keyWordInput" type="text" placeholder="輸入關鍵字" id="keyWord" name="keyWord">
                            </div>
                            <div class="areaText" id="selectArea" data-toggle="modal" data-target="#areaDialogBox">
                                <span id="selectedAreaText">
                                    地區
                                </span>
                                <input type="text" id="areaValue" class="d-none" name="area">
                                <div class="moreBtnImg">
                                </div>
                            </div>
                            <div class="classText" id="selectClass" data-toggle="modal" data-target="#classDialogBox">
                                <span id="selectedCassText">
                                    類別
                                </span>
                                <input type="text" id="jobClassValue" class="d-none" name="jobClass">
                                <div class="moreBtnImg">
                                </div>
                            </div>

                            <button class="searchBtn" id="searchBtnComfirm"></button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
        <!-- ----- -->

        <div class="container">
            <!-- 標題列 -->
            <div class="row searchTitleBar">
                <div class="col-4">實習職稱</div>
                <div class="col-3">公司名稱</div>
                <div class="col-2">地點</div>
                <div class="col-2">更新時間 <span>^</span></div>
                <div class="col-1"></div>
            </div>
            <!-- --- -->


            <!-- 一組 實習職缺 card -->
            <% if( searchList.length == 0){ %>
            <div class="notFind"></div>
            <% }else{ %>
            <% searchList.forEach(function(searchResult){ %>
            <div class="row searchResultCard">
                <div class="col-lg-4">
                    <div class="jobClessShow"><%= searchResult.jobClassName %></div>
                    <div><%= searchResult.jobName %></div>
                </div>
                <div class="col-lg-3"><%= searchResult.companyName %></div>
                <div class="col-lg-2"><%= searchResult.cityName %><%= searchResult.townshipName %></div>
                <div class="col-lg-2"><%= searchResult.jobAddtime %></div>
                <div class="col-lg-1">
                    <div class="applyBtn">
                        <a href="#">
                            <div class="applyBtnTop">
                                <div class="applyBtnPaper"></div>
                                <a href="/job?id=<%= searchResult.jobId %>">
                                    <div class="applyBtnText">應徵</div>
                                </a>

                            </div>
                            <div class="applyBtnSec"></div>
                        </a>
                    </div>
                </div>
            </div>
            <% }) %>

            <% } %>
            <!-- ---------- -->

        </div>


        <!-- 地區選擇對話盒 -->
        <div class="modal fade" id="areaDialogBox" tabindex="-1" role="dialog"
            aria-labelledby="exampleModalScrollableTitle" aria-hidden="true">

            <div class="modal-dialog modal-dialog-scrollable" role="document">
                <div class="modal-content">

                    <div class="modal-header">
                        <h5 class="modal-title" id="exampleModalScrollableTitle">地區選擇</h5>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>

                    <div class="modal-body overflow-hidden">

                        <div class="cityItems">
                            <div class="nav flex-column" id="v-pills-tab" role="tablist" aria-orientation="vertical">

                                <div>縣市</div>

                                <!-- 一組縣市 -->
                                <% cityList.forEach(function(cityList){ %>
                                <a class="nav-link cityNavItem" id="city<%= cityList.cityId %>" data-toggle="pill"
                                    href="#area<%= cityList.cityId %>" role="tab"
                                    aria-selected="true"><%= cityList.cityName %></a>
                                <% }) %>
                                <!-- 一組縣市^ -->

                            </div>
                        </div>

                        <div class="areaItem ">
                            <div class="tab-content" id="v-pills-tabContent">

                                <div>
                                    區
                                </div>

                                <!-- 一組區 -->
                                <% cityList.forEach(function(cityList){  
                                var citySelect = cityList.cityId;
                                
                                var filterTowns = townsList.filter(function(item,index,array){
                                 return (citySelect-1 < item.cityId) && (item.cityId< citySelect + 1);   
                                 })
                               
                               %>
                                <div class="tab-pane fade " id="area<%= cityList.cityId %>" role="tabpanel">
                                    <% filterTowns.forEach(function(towns){
                 
                               %>
                                    <div><input type="checkbox" id="town<%= towns.townshipId %>" name="areaSelected"
                                            value="<%= cityList.cityName %><%= towns.townshipName %>"
                                            data-areaValue="<%= towns.townshipId %>"><label
                                            for="town<%= towns.townshipId %>"><%= towns.townshipName%></label></div>
                                    <% })  %>
                                </div>
                                <% }) %>
                                <!-- 一組區^ -->



                            </div>

                        </div>

                    </div>

                    <div class="modal-footer">
                        <button type="button" class="btn btn-outline-common" data-dismiss="modal"
                            id="areaCancelBtn">Close</button>
                        <button type="button" class="btn btn-common" data-dismiss="modal" id="areaComfirmBtn">Save
                        </button>
                    </div>
                </div>

            </div>
        </div>
        <!-- 地區選擇對話盒^ -->



        <!-- 職缺類別選擇對話盒 -->
        <div class="modal fade" id="classDialogBox" tabindex="-1" role="dialog"
            aria-labelledby="exampleModalScrollableTitle" aria-hidden="true">

            <div class="modal-dialog modal-dialog-scrollable" role="document">
                <div class="modal-content">

                    <div class="modal-header">
                        <h5 class="modal-title" id="exampleModalScrollableTitle">職務類別選擇</h5>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>

                    <div class="modal-body ">
                        <div class="classCheckBoxLay">
                            <div>職務類別</div>
                            <% jobClassList.forEach(function(jobClass){ %>
                            <div class="classCheckBoxItem">
                                <input type="checkbox" id="jobClass<%= jobClass.jobClassId %>" name="jobClass"
                                    value="<%= jobClass.jobClassName %>"
                                    data-jobClassValue="<%= jobClass.jobClassId %>"><label
                                    for="jobClass<%= jobClass.jobClassId %>"><%= jobClass.jobClassName %></label>
                            </div>
                            <% }) %>
                        </div>

                    </div>


                    <div class="modal-footer">
                        <button type="button" class="btn btn-outline-common" data-dismiss="modal"
                            id="clsseCancelBtn">Close</button>
                        <button type="button" class="btn btn-common" data-dismiss="modal"
                            id="clsseComfirmBtn">Save</button>
                    </div>

                </div>

            </div>
        </div>
        <!-- 職缺類別對話盒^ -->


        <br>
        <br>
        <br>
        <br>
        <br>
        <br>
        <br>
        <br>
        <br>
        <br>
        <br>
        <br>
        <br>
        <br>
        <br>
        <br>
        <br>
        <br>
        <br>
        <br>
        <!-- Footer Section Start -->
        <footer class="footer-area section-padding">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <div class="footer-text text-center wow fadeInDown" data-wow-delay="0.3s">
                            <ul class="social-icon">
                                <li>
                                    <a class="facebook" href="#"><i class="icon-social-facebook"></i></a>
                                </li>
                                <li>
                                    <a class="twitter" href="#"><i class="icon-social-twitter"></i></a>
                                </li>
                                <li>
                                    <a class="instagram" href="#"><i class="icon-social-instagram"></i></a>
                                </li>
                                <li>
                                    <a class="instagram" href="#"><i class="icon-social-linkedin"></i></a>
                                </li>
                                <li>
                                    <a class="instagram" href="#"><i class="icon-social-google"></i></a>
                                </li>
                            </ul>
                            <p>實習趣. 版權所有</p>

                        </div>
                    </div>
                </div>
            </div>
        </footer>
        <script src="assets/js/jquery-min.js"></script>
        <script src="assets/js/popper.min.js"></script>
        <script src="assets/js/bootstrap.min.js"></script>
        <script src="assets/js/jquery.mixitup.js"></script>
        <script src="assets/js/jquery.counterup.min.js"></script>
        <script src="assets/js/waypoints.min.js"></script>
        <script src="assets/js/wow.js"></script>
        <script src="assets/js/jquery.nav.js"></script>
        <script src="assets/js/jquery.easing.min.js"></script>
        <script src="assets/js/nivo-lightbox.js"></script>
        <script src="assets/js/jquery.slicknav.js"></script>
        <script src="assets/js/main.js"></script>
        <script src="assets/js/form-validator.min.js"></script>
        <script src="assets/js/contact-form-script.min.js"></script>
        <script src="assets/js/map.js"></script>
        <script src="js/ajaxrequest/searchquery.js"></script>
</body>

