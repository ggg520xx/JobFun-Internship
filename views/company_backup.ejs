  <div class="modal fade" id="areaDialogBox" tabindex="-1" role="dialog" aria-labelledby="exampleModalScrollableTitle"
    aria-hidden="true">

    <div class="modal-dialog modal-dialog-scrollable" role="document">
      <div class="modal-content">

        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalScrollableTitle">地區選擇</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>

        <div class="modal-body overflow-hidden">

          <div class="cityItems">
            <div class="nav flex-column" id="v-pills-tab" role="tablist" aria-orientation="vertical">

              <div>縣市</div>

              <!-- 一組縣市 -->
              <% cityList.forEach(function(cityList){ %>
              <a class="nav-link cityNavItem" id="city<%= cityList.cityId %>" data-toggle="pill"
                href="#area<%= cityList.cityId %>" role="tab" aria-selected="true"><%= cityList.cityName %></a>
              <% }) %>
              <!-- 一組縣市^ -->

            </div>
          </div>

          <div class="areaItem ">
            <div class="tab-content" id="v-pills-tabContent">

              <div>
                區
              </div>

              <!-- 一組區 -->
              <% cityList.forEach(function(cityList){  
                          var citySelect = cityList.cityId;
                          
                          var filterTowns = townsList.filter(function(item,index,array){
                           return (citySelect-1 < item.cityId) && (item.cityId< citySelect + 1);   
                           })
                         
                         %>
              <div class="tab-pane fade " id="area<%= cityList.cityId %>" role="tabpanel">
                <% filterTowns.forEach(function(towns){
           
                         %>
                <div><input style="display:inline-block" type="radio" id="town<%= towns.townshipId %>"
                    name="areaSelectForAdd" value="<%= cityList.cityName %><%= towns.townshipName %>"
                    data-areaValue="<%= towns.townshipId %>"><label for="town<%= towns.townshipId %>"
                    style="display:inline-block"><%= towns.townshipName%></label></div>
                <% })  %>
              </div>
              <% }) %>
              <!-- 一組區^ -->



            </div>

          </div>

        </div>

        <div class="modal-footer">
          <button type="button" class="btn btn-outline-common" data-dismiss="modal" id="areaCancelBtn">Close</button>
          <button type="button" class="btn btn-common" data-dismiss="modal" id="areaBtnForAdd">Save
          </button>
        </div>
      </div>

    </div>
  </div>
  <!-- 地區選擇對話盒^ -->



  <!-- 職缺類別選擇對話盒for新增 -->
