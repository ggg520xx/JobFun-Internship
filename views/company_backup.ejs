                  <script>

                    // 254,103,103
                    // 105,166,88

                    // color switch
                    // function switch_color() {
                    // document.getElementById('info_switch').style.color = "rgb(254, 103, 103)";

                    var i = 0;
                    function changeColor(i_color) {
                      console.log(i_color);

                      console.log($(i_color));
                      console.log($(i_color).find('u'));
                      // jquery 底下的值 find 取

                      // $(i_color).find('u').css('color','yellow');
                      if (i++ % 2 == 0) {
                        $(i_color).find('u').css('color', 'rgb(227, 82, 82)');
                        $(i_color).find('u').text('目前下架中');
                      } else {
                        $(i_color).find('u').css('color', 'rgb(76, 166, 51)');
                        $(i_color).find('u').text('目前上架中');
                      }
                      // if (i++ % 2 == 0) {
                      //   document.getElementById('info_switch').style.color = 'red';
                      // } else {
                      //   document.getElementById('info_switch').style.color = 'black';
                      // }
                    }






                    // 同步互換～～～～～～～
                    // 上面綁id 和 onclick 


                    // 點擊去抓div 把hidden改為false
                    function createjob_Function() {
                      var scj = document.getElementById("show_create_job");
                      var hd = document.getElementById("job_page");

                      var hidebtn = document.getElementById('check_click');
                      hidebtn.style.display = 'none';

                      var filterbtn = document.getElementById('filter_click');
                      filterbtn.style.display = 'none';

                      scj.hidden = false;
                      hd.hidden = true;
                    }


                    function check_back_Function() {
                      var scjh = document.getElementById("show_create_job");
                      var hds = document.getElementById("job_page");
                      var editjob = document.getElementById("edit_job");

                      var hidebtn = document.getElementById('check_click');
                      hidebtn.style.display = 'inline-block';

                      var filterbtn = document.getElementById('filter_click');
                      filterbtn.style.display = 'inline-block';

                      editjob.hidden = true;
                      scjh.hidden = true;
                      hds.hidden = false;
                    }

                    function edit_job_Function() {
                      var editjob = document.getElementById("edit_job");
                      var v = document.getElementById("job_page");


                      var hidebtn = document.getElementById('check_click');
                      hidebtn.style.display = 'none';

                      var filterbtn = document.getElementById('filter_click');
                      filterbtn.style.display = 'none';

                      editjob.hidden = false;
                      v.hidden = true;
                    }
